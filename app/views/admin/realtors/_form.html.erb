<%= form_for([:admin, realtor]) do |f| %>
  <%= form_errors(realtor) %>

  <div class="field">
    <%= f.label :first_name %><br>
    <%= f.text_field :first_name %>
  </div>
  <div class="field">
    <%= f.label :last_name %><br>
    <%= f.text_field :last_name %>
  </div>
  <div class="field">
    <%= f.label :email %><br>
    <%= f.text_field :emails %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
